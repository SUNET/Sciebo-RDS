    <author>Peter Heiss</author>
    <version>0.1</version>
    <namespace>RDS</namespace>
    <category>integration</category>
    <dependencies>
        <owncloud min-version="10" max-version="10" />
    </dependencies>
    <website>https://www.research-data-services.org</website>
    <repository type="git">https://github.com/Sciebo-RDS/Sciebo-RDS</repository>
    <bugs>https://github.com/Sciebo-RDS/Sciebo-RDS/issues</bugs>
    <settings>
        <personal>OCA\RDS\Panels\PersonalPanel</personal>
        <admin>OCA\RDS\Panels\AdminPanel</admin>
    </settings>
     <settings-sections>
        <personal>OCA\RDS\Panels\PersonalSection</personal>
        <admin>OCA\RDS\Panels\AdminSection</admin>
    </settings-sections>
